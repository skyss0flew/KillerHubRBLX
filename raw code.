-- Scripts:

local function LDZYVKI_fake_script() -- ResetPlayer.LocalScript 
	local script = Instance.new('LocalScript', ResetPlayer)

	script.Parent.MouseButton1Click:Connect(function(plr)
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
	end)
end
coroutine.wrap(LDZYVKI_fake_script)()
local function RFCLIZ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function(plr)
		game.Players.LocalPlayer.Character.Humanoid.Health = 999999
	end)
end
coroutine.wrap(RFCLIZ_fake_script)()
local function FFVNG_fake_script() -- PlayerFrame.Script 
	local script = Instance.new('Script', PlayerFrame)

	frame = script.Parent.PlayerFrame
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(FFVNG_fake_script)()
local function QQJN_fake_script() -- GameFrame.Script 
	local script = Instance.new('Script', GameFrame)

	frame = script.Parent.GameFrame
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(QQJN_fake_script)()
local function TGDG_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local mt = getrawmetatable(game)
	local onc = mt.__namecall
	_G.Multiplier = 5
	setreadonly(mt,false)
	local tog = false
	mt.__namecall = function(...)
		local t={...}
		if getnamecallmethod() == 'FireServer' then
			for i,v in pairs({...}) do
				if tostring(v) == 'EngineEvent'  then
					t[2] = t[2] * _G.Multiplier
					t[3] = t[3]+Vector3.new(0,0,(t[3].Z*_G.Multiplier)-t[3].Z)  
					print('edited')
					break;
				end
			end
		end
		return onc(unpack(t))
	end
	Copy

end
coroutine.wrap(TGDG_fake_script)()
